    "def next_c(a, n):\n",
    "    r = len(a)\n",
    "    i = r - 1\n",
    "    while a[i] == n - r + i + 1:\n",
    "        i -= 1\n",
    "    a[i] = a[i] + 1\n",
    "    j = i + 1\n",
    "    while j < r:\n",
    "        a[j] = a[j - 1] + 1\n",
    "        j += 1\n",
    "    return a"
   ]
